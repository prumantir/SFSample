@{
    ViewBag.Title = "Presentation";
    ViewData["ProductName"]="Presentation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Presentation Features:</h2>
<br/>
<li> Default</li>
<br/>
<div id = "ControlRegion">
@{Html.BeginForm("PresentationFeatures", "Presentation", FormMethod.Post);
{
    <div class="rowdiv">
        <label>
            Click the button to view the Presentation file generated by Essential Presentation. Please
            note that PowerPoint viewer is required to view the resultant file.
        </label>
    </div>
    <br />
    <div class="rowdiv">
        <div class="celldiv">
            <input class="buttonStyle" type="submit"
                   value="Generate" style="width:163px;" name="button" />
        </div>
    </div>
    Html.EndForm();
}}
</div>
